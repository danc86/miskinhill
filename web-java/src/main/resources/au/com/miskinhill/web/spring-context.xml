<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

    <bean class="au.com.miskinhill.rdf.ModelFactory" factory-method="load" lazy-init="true">
        <constructor-arg value="/home/dan/miskinhill/content/meta.xml" />
    </bean>

    <context:component-scan base-package="au.com.miskinhill.rdftemplate.datatype" />
    <context:component-scan base-package="au.com.miskinhill.web" />
    
</beans>